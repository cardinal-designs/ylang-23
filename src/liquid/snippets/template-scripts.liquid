{%- if template.name == 'product' %}
  <script type="application/ld+json">
    {
      "@context" : "https://schema.org",
      "@type" : "Product",
      "@id" : {{ shop.url | append: product.url | json }},
      "url" : {{ shop.url | append: product.url | json }},
      "name" : {{ product.title | json }},
      "image" : {{ product.featured_image | json }},
      "description" : {{ product.description | json }},
      "sku" : {{ product.selected_or_first_available_variant.sku | json }},
      "brand" : {
        "@type": "Brand",
        "name": {{ product.vendor | json }}
      }
      {%- if settings.review_source == 'shopify' and product.metafields.spr.reviews -%}
        ,"aggregateRating": {
        "@type": "AggregateRating",
        "ratingValue": {{ product.metafields.spr.reviews | split: '"ratingValue": "' | last | split: '"' | first | plus: 0 }},
        "ratingCount": {{ product.metafields.spr.reviews | split: '"reviewCount": "' | last | split: '"' | first | plus: 0 }}
        }
      {%- endif -%}
    }
  </script>
{%- endif -%}

